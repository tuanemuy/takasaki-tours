{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env", ".env.local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build", "^generate", "generate"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "dependsOn": ["^generate", "generate"],
      "cache": false,
      "persistent": true
    },
    "generate": {
      "cache": false
    }
  }
}
